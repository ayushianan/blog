<p id="notice"><%= notice %></p>

<h1>Listing Articles</h1>

<table>
 <thead>
   <tr>
     <th>Title</th>
     <th>Description</th>
     <th colspan="3"></th>
   </tr>
 </thead>

 <tbody>
   <% @articles.each do |article| %>
     <tr>
       <td><%= article.title %></td>
       <td><%= article.text%></td>
       
     </tr>
   <% end %>
 </tbody>
</table>

<br>


